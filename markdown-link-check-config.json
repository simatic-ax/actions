{
    "replacementPatterns": [
      {
        "pattern": "^/",
        "replacement": "/github/workspace/"
      },
      {
        "pattern": "^../",
        "replacement": "/github/workspace/"
      },
      {
        "pattern": "^./",
        "replacement": "/github/workspace/"
      }
    ],
    "ignorePatterns": [
      {
        "pattern": "^.git/"
      },
      {
        "pattern": "./actions-test/.*"
      }
    ],
    "timeout": "20s",
    "retryOn429": true,
    "retryCount": 5,
    "fallbackRetryDelay": "30s",
    "aliveStatusCodes": [200, 206],
    "markdown": {
      "ignoreLinks": [
        {
          "pattern": "^#"
        }
      ]
    }
  }